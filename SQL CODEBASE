/* criar uma tabela /*

CREATE TABLE PRIMEIRA_TABELA(
    codigo number NOT NULL,
    nome varchar(150) NOT NULL,
    cidade varchar(150) NOT NULL,
    estado varchar(2) NOT NULL,
    cep number NOT NULL);

/* rodar a tabela /*

SELECT * FROM all_tab_columns WHERE table_name = 'PRIMEIRA_TABELA'

/* fazer uma modificação /*

ALTER TABLE PRIMEIRA_TABELA
MODIFY (cep number(8));
